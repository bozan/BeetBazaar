<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="passwordUnregisteredVM" type="com.beyzaozansapplication.app.modules.passwordunregistered.data.viewmodel.PasswordUnregisteredVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearPasswordunregi"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameImageplacehold"
            android:layout_height="match_parent" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image_800x390}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image_800x390"/>
            <LinearLayout android:id="@+id/linearColumnpagetitle"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle_2">
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStacklabel"
                    android:layout_height="@dimen/_142pxv" android:layout_width="match_parent">
                    <LinearLayout android:id="@+id/linearColumnlabel"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLabel"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{passwordUnregisteredVM.passwordUnregisteredModel.txtLabel}"
                            style="@style/txtGothicA1medium16" tools:text="@string/lbl_e_mail"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_100_radius_24"
                            android:ellipsize="end"
                            android:hint="@string/lbl_e_mail"
                            android:id="@+id/etBaseinputfie"
                            android:inputType="textEmailAddress"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:paddingBottom="@dimen/_17pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_16pxh"
                            android:paddingTop="@dimen/_14pxv"
                            android:singleLine="true"
                            android:text="@={passwordUnregisteredVM.passwordUnregisteredModel.etBaseinputfieValue}"
                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearNavigationbar"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStyle">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearLargetitlebar"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStyle">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtPagetitle"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_38pxv"
                                android:layout_marginEnd="@dimen/_16pxh"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_9pxv"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{passwordUnregisteredVM.passwordUnregisteredModel.txtPagetitle}"
                                style="@style/txtGothicA1gothica1semibold26" tools:text="@string/lbl_register"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLabelOne"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_35pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{passwordUnregisteredVM.passwordUnregisteredModel.txtLabelOne}"
                    style="@style/txtGothicA1medium16" tools:text="@string/lbl_name"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_100_radius_24"
                    android:ellipsize="end"
                    android:hint="@string/lbl_name"
                    android:id="@+id/etBaseinputfieOne"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={passwordUnregisteredVM.passwordUnregisteredModel.etBaseinputfieOneValue}"
                    android:textColorHint="@color/black_900"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLabelTwo"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{passwordUnregisteredVM.passwordUnregisteredModel.txtLabelTwo}"
                    style="@style/txtGothicA1medium16" tools:text="@string/lbl_surname"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_100_radius_24"
                    android:ellipsize="end"
                    android:hint="@string/lbl_surname"
                    android:id="@+id/etBaseinputfieTwo"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={passwordUnregisteredVM.passwordUnregisteredModel.etBaseinputfieTwoValue}"
                    android:textColorHint="@color/black_900"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLabelThree"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{passwordUnregisteredVM.passwordUnregisteredModel.txtLabelThree}"
                    style="@style/txtGothicA1medium16" tools:text="@string/lbl_address"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_100_radius_24"
                    android:ellipsize="end"
                    android:hint="@string/lbl_address"
                    android:id="@+id/etBaseinputfieThree"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@={passwordUnregisteredVM.passwordUnregisteredModel.etBaseinputfieThreeValue}"
                    android:textColorHint="@color/black_900"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLabelFour"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{passwordUnregisteredVM.passwordUnregisteredModel.txtLabelFour}"
                    style="@style/txtGothicA1medium16" tools:text="@string/lbl_password"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_100_radius_24"
                    android:ellipsize="end"
                    android:hint="@string/lbl_password"
                    android:id="@+id/etBaseinputfieFour"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@={passwordUnregisteredVM.passwordUnregisteredModel.etBaseinputfieFourValue}"
                    android:textColorHint="@color/black_900"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnContinue"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_46pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_24pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_24pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_continue"
                    android:textAllCaps="false"
                    style="@style/btnGradientRounded" tools:text="@string/lbl_continue"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLoremipsumdol"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_37pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_80pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{passwordUnregisteredVM.passwordUnregisteredModel.txtLoremipsumdol}"
                    style="@style/txtGothicA1regular14_1" tools:text="@string/lbl_beetbazaar"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
